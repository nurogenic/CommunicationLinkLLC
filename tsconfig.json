{
"extends": "astro/tsconfigs/strict",
"compilerOptions": {
    "baseUrl": "src",
    "strictNullChecks": true,
    "types": ["@cloudflare/workers-types/2023-07-01", "astro/client"],
    "paths": {
      "@lib/*": ["lib/*"],
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["assets/*"],
      "@pages/*": ["pages/*"]
    }
}
}